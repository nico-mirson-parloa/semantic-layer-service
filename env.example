# Databricks Configuration (Required)
DATABRICKS_HOST=your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-personal-access-token
DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/your-warehouse-id

# Databricks Genie Configuration (Optional but recommended)
# To get this: Go to your Genie room in Databricks, look at the URL
# Example URL: https://your-workspace.cloud.databricks.com/genie/rooms/abc123def456
# Your space ID would be: abc123def456
DATABRICKS_GENIE_SPACE_ID=your-genie-space-id

# API Configuration (Optional)
DEBUG=false
